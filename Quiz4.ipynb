    "x, y = 0,0\n",
    "for x in range(10000):\n",
    "    dice = numpy.random.random(2)\n",
    "    dice *= 6\n",
    "    dice = numpy.ceil(dice)\n",
    "    if dice.sum() == 2:\n",
    "        x += 1\n",
    "    elif dice.sum() == 7:\n",
    "        y += 1\n",
    "print(x, y)\n"
